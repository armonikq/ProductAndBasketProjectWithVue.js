{"ast":null,"code":"export default {\n  data() {\n    return {\n      city: '',\n      loading: true,\n      weather: '',\n      temperature: '',\n      wind: '',\n      humidity: ''\n    };\n  },\n  created() {\n    this.getWeatherData();\n  },\n  methods: {\n    getWeatherData() {\n      const apiKey = 'c0dda1e473ad2a57d5290d094c274381'; // OpenWeatherMap API key\n      const apiUrl = `https://api.openweathermap.org/data/2.5/weather?q=${this.city}&appid=${apiKey}&units=metric`;\n      fetch(apiUrl).then(response => response.json()).then(data => {\n        this.loading = false;\n        this.city = data.city[0];\n        this.weather = data.weather[0].description;\n        this.temperature = data.main.temp;\n        this.wind = data.wind.speed;\n        this.humidity = data.main.humidity;\n      }).catch(error => {\n        console.error('Hava durumu verileri alınırken bir hata oluştu:', error);\n        this.loading = false;\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","city","loading","weather","temperature","wind","humidity","created","getWeatherData","methods","apiKey","apiUrl","fetch","then","response","json","description","main","temp","speed","catch","error","console"],"sources":["/Users/furkan/vue-deneme/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"la\">\n    <h1>{{ city }} Hava durumu</h1>\n    <div v-if=\"loading\">Yükleniyor...</div>\n    <div v-else>\n      <p>{{ weather }}</p>\n      <p>{{ temperature }}°C</p>\n      <p>Rüzgar: {{ wind }} km/s</p>\n      <p>Nem: {{ humidity }}%</p>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      city: '',\n      loading: true,\n      weather: '',\n      temperature: '',\n      wind: '',\n      humidity: ''\n    };\n  },\n  created() {\n    this.getWeatherData();\n  },\n  methods: {\n    getWeatherData() {\n      const apiKey = 'c0dda1e473ad2a57d5290d094c274381'; // OpenWeatherMap API key\n      const apiUrl = `https://api.openweathermap.org/data/2.5/weather?q=${this.city}&appid=${apiKey}&units=metric`;\n\n      fetch(apiUrl)\n          .then(response => response.json())\n          .then(data => {\n            this.loading = false;\n            this.city = data.city[0]\n            this.weather = data.weather[0].description;\n            this.temperature = data.main.temp;\n            this.wind = data.wind.speed;\n            this.humidity = data.main.humidity;\n          })\n          .catch(error => {\n            console.error('Hava durumu verileri alınırken bir hata oluştu:', error);\n            this.loading = false;\n          });\n    }\n  }\n};\n</script>\n\n\n"],"mappings":"AAaA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACPD,cAAcA,CAAA,EAAG;MACf,MAAME,MAAK,GAAI,kCAAkC,EAAE;MACnD,MAAMC,MAAK,GAAK,qDAAoD,IAAI,CAACV,IAAK,UAASS,MAAO,eAAc;MAE5GE,KAAK,CAACD,MAAM,EACPE,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACb,IAAG,IAAK;QACZ,IAAI,CAACE,OAAM,GAAI,KAAK;QACpB,IAAI,CAACD,IAAG,GAAID,IAAI,CAACC,IAAI,CAAC,CAAC;QACvB,IAAI,CAACE,OAAM,GAAIH,IAAI,CAACG,OAAO,CAAC,CAAC,CAAC,CAACa,WAAW;QAC1C,IAAI,CAACZ,WAAU,GAAIJ,IAAI,CAACiB,IAAI,CAACC,IAAI;QACjC,IAAI,CAACb,IAAG,GAAIL,IAAI,CAACK,IAAI,CAACc,KAAK;QAC3B,IAAI,CAACb,QAAO,GAAIN,IAAI,CAACiB,IAAI,CAACX,QAAQ;MACpC,CAAC,EACAc,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,iDAAiD,EAAEA,KAAK,CAAC;QACvE,IAAI,CAACnB,OAAM,GAAI,KAAK;MACtB,CAAC,CAAC;IACR;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}